users to our test user\n","trial_user = 5\n","trial_user_rating_df = user_rating_click.loc[trial_user,:] #taking user 5 as trial user\n","rest_user_rating_df = user_rating_click.drop(trial_user)\n","\n","import numpy as np\n","def cosine_similarity(v,u):\n","    return (v @ u)/ (np.linalg.norm(v) * np.linalg.norm(u))\n"," \n","\n","similarity = cosine_similarity(vh[:,trial_user], vh)\n","n=4\n","arr=similarity\n","indices = np.argpartition(arr, n-1)[:n]\n","indices = np.sort(indices)[0:3]\n","art_colab =[]\n","\n","#Iterating through these 3 users, we find the top rated articles for each of the 3 users and form our corpus\n","for i in indices:\n","  array = rest_user_rating_df.values[i,:]\n","  itemindex = (array>1).nonzero()[0]\n","  for j in itemindex: \n","    art_colab.append(j)"],"metadata":{"id":"TsrSX1n7obTM"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["indices"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"n0Iif4BTo27G","executionInfo":{"status":"ok","timestamp":1650823098031,"user_tz":-330,"elapsed":8,"user":{"displayName":"Dheer Mankad","userId":"11542503690888670879"}},"outputId":"29bcc434-758f-45b9-e547-6a1644b169d6"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([2, 7, 8])"]},"metadata":{},"execution_count":16}]},{"cell_type":"code","source":["#From the corpus of best rated articles, we choose 10 articles randomly to reduce bias\n","art_colab = random.sample(art_colab, 10)\n","data.loc[art_colab]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":875},"id":"W7V4mE-0xm4K","executionInfo":{"status":"ok","timestamp":1650823099819,"user_tz":-330,"elapsed":4,"user":{"displayName":"Dheer Mankad","userId":"11542503690888670879"}},"outputId":"82c35c49-115b-47ac-e96b-a7e0243734e7"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["           Category            Sub-Category  \\\n","2372  entertainment              